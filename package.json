{
  "name": "redux-playground",
  "version": "0.0.1",
  "description": "Redux playground",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf lib",
    "predev": "npm run build:shell",
    "dev": "babel-node src/index",
    "prebuild": "npm run clean",
    "build": "babel src --out-dir lib",
    "build:shell": "(cd src/Shell && webpack)",
    "start": "nodemon --watch src -e js,jsx --ignore '**/dist/*.js' --exec npm run dev",
    "serve": "node lib/index"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "dependencies": {
    "express": "^4.13.3",
    "fluxible": "^1.0.3",
    "fluxible-addons-react": "^0.2.2",
    "lodash": "^3.10.1",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-redux": "^4.0.2",
    "redux": "^3.0.4",
    "redux-thunk": "^1.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.21",
    "babel-loader": "^6.2.0",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "nodemon": "^1.8.1",
    "webpack": "^1.12.9"
  }
}
